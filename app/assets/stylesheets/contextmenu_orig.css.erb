#hero a.button {
    display: inline-block;
    text-decoration: none;
    vertical-align: middle;
    *vertical-align: auto;
    *zoom: 1;
    *display: inline;
    padding: 29px 58px 27px 58px;
    font-size: 15px;
    color: #fff;
    margin: 0 auto;
    -moz-transition-property: all;
    -o-transition-property: all;
    -webkit-transition-property: all;
    transition-property: all;
    -moz-transition-duration: 0.4s;
    -o-transition-duration: 0.4s;
    -webkit-transition-duration: 0.4s;
    transition-duration: 0.4s;
    text-shadow: rgba(0, 0, 0, 0.25) 0 -1px 0;
    background-color: #f5756c;
    background: url('data:image/svg+xml;base64,'), #f5756c;
    background: -webkit-gradient(linear, 50% 0%, 50% 100%, color-stop(0%, #f7766c), color-stop(100%, #f66359)), #f5756c;
    background: -moz-linear-gradient(#f7766c, #f66359), #f5756c;
    background: -webkit-linear-gradient(#f7766c, #f66359), #f5756c;
    background: linear-gradient(#f7766c, #f66359), #f5756c;
    -moz-box-shadow: inset rgba(255, 255, 255, 0.5) 0 1px 0, rgba(0, 0, 0, 0.3) 0 1px 2px;
    -webkit-box-shadow: inset rgba(255, 255, 255, 0.5) 0 1px 0, rgba(0, 0, 0, 0.3) 0 1px 2px;
    box-shadow: inset rgba(255, 255, 255, 0.5) 0 1px 0, rgba(0, 0, 0, 0.3) 0 1px 2px;
    -moz-border-radius: 4px;
    -webkit-border-radius: 4px;
    border-radius: 4px; }

#hero a.button:hover {
    -moz-box-shadow: inset rgba(255, 255, 255, 0.3) 0 1px 0, rgba(0, 0, 0, 0.3) 0 1px 2px, #f5756c 0 1px 40px;
    -webkit-box-shadow: inset rgba(255, 255, 255, 0.3) 0 1px 0, rgba(0, 0, 0, 0.3) 0 1px 2px, #f5756c 0 1px 40px;
