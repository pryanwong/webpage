%table{:class=>"flatTable"}
   %tr{:class=>"titleTr"}
      %td{:class=>"titleTd"}Users
      %td{:colspan=>"4"}
   %tr{:class=>"headingTr"}
      %th= sortable2 "email"
      %th= "Switch"
      %th{:colspan=>"3"}
   %tbody
      -@users.each do |user|
         %tr
            %td= user.email
            %td= link_to image_tag("email-icon.png"), company_user_switchuser_path(user.company_id ,user.id)
            %td{:colspan=>"3"}
   =will_paginate @users
